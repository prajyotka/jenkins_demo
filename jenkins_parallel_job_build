pipeline {
    agent any
    stages {
        stage ('initial sleep time') {
            steps {
                sleep 120
            }
        }
        stage ('paralle job') {
            parallel {
                stage ('build1'){
                    steps {
                        echo 'running job 1'
                        echo 'first job'
                    }
                }
                stage ('build2') {
                    steps {
                        echo 'running job 2'
                        echo 'second job'
                    }
                }
            }
        }
    }
}